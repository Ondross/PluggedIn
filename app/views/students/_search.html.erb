<style>
  table.search td{height: 19px;}</style>
<table class= "search">
	
		<%= form_tag :search do %>
	    <tr><td><%= text_field_tag "name", "" %></td></tr><tr><td>
		<%= text_field_tag "address", "" %></td></tr><tr><td>
		<%= text_field_tag "home_phone", "" %></td></tr><tr><td>
		<%= text_field_tag "cell", "" %></td></tr><tr><td>
		<%= text_field_tag "dad_work", "" %></td></tr><tr><td>
		<%= text_field_tag "mom_work", "" %></td></tr><tr><td>
		<%= text_field_tag "dad_cell", "" %></td></tr><tr><td>
		<%= text_field_tag "mom_cell", "" %></td></tr><tr><td>
		<%= text_field_tag "dob", "" %></td></tr><tr><td>
		<%= text_field_tag "instruments_played", "" %></td></tr><tr><td>
		<%= text_field_tag "sessions_participated_in", "" %></td></tr><tr><td>
		<%= text_field_tag "parent_names", "" %></td></tr><tr><td>
		    <% [ 'And', 'or' ].each do |andor| %>
     <%= radio_button_tag 'andor', andor, @andor == andor %>
      <%= andor.humanize %>
    <% end %>
		<%= submit_tag "Go" %>
	<% end %></td></tr>
	</table>